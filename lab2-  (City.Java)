public class City {
    private String name;
    private int population;
    private String country;
    private String area;
    private Boolean seaside;
    
    
    public void info(){
        System.out.println("Name: "+name + "population:" + population +
                "country:" + country +"area:" + area + "seaside:"+ seaside);
    }
    public City(){
        
    }
    public City(String name,int population,String country,String area,
            Boolean seaside) {
        this.name =  name ;
        this.population = population ;
        this.country= country;
        this.area= area;
        this.seaside= seaside;
        
    }

    public String getName() {
        return name;
    }

    public int getPopulation() {
        return population;
    }

    public String getCountry() {
        return country;
    }

    public String getArea() {
        return area;
    }

    public Boolean getSeaside() {
        return seaside;
    }
    public void setName(String name) {
        this.name = name;
    }
    public void setPopulation(int population) {
        this.population = population;
    }

    public void setCountry(String country) {
        this.country = country;
    }

    public void setArea(String area) {
        this.area = area;
    }

    public void setSeaside(Boolean seaside) {
        this.seaside = seaside;
    }
     @Override
    public String toString() {
        return "City{" + "name=" + name +  ", population=" + population + ", country=" + country + ", area=" + area + ", seaside=" + seaside + '}';
    }

    }
    

